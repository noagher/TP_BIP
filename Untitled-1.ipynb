   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def X(n,x0,beta,alpha,x1):\n",
    "    l=[x0,x1]\n",
    "    k=0\n",
    "    while( k<=n):\n",
    "        l.append(alpha*l[-1]+beta*l[-2])\n",
    "        k=k+1\n",
    "    return l[:-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [],
   "source": [
    "alpha=-5\n",
    "beta=5\n",
    "\n",
    "l=X(100,9,beta,alpha,9)"
   ]
